@article{Ghermandi2018,
abstract = {Social media provide a wealth of behavioral data that can be used to investigate the provision of environmental services. In this study, the preferences revealed by photo-sharing social media users are analyzed through travel cost modeling to infer the monetary value of recreation in 115 man-made wetland ecosystems. Photographs' metadata and other publicly available information are used to determine the frequency of recreational trips and the home location of visitors. The mean willingness to pay for access to 115 wetlands is found to range between {\$}5.3 and {\$}374 (2015 international {\$}). The comparison of estimated recreational benefits with the capital, operation and maintenance costs of 74 wetlands reveals that such benefits are considerable and should be taken into account in the design and management of these systems. The approach demonstrates the potential for ecosystem service valuation techniques to incorporate the large amounts of behavioral data available from online resources.},
author = {Ghermandi, Andrea},
doi = {10.1016/j.ecoser.2017.12.012},
file = {:C$\backslash$:/Users/Max/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ghermandi - 2018 - Integrating social media analysis and revealed preference methods to value the recreation services of ecologically en.pdf:pdf},
issn = {22120416},
journal = {Ecosystem Services},
pages = {351--357},
publisher = {Elsevier B.V.},
title = {{Integrating social media analysis and revealed preference methods to value the recreation services of ecologically engineered wetlands}},
url = {https://doi.org/10.1016/j.ecoser.2017.12.012},
volume = {31},
year = {2018}
}
@article{Wood2013,
abstract = {Scientists have traditionally studied recreation in nature by conducting surveys at entrances to major attractions such as national parks. This method is expensive and provides limited spatial and temporal coverage. A new source of information is available from online social media websites such as flickr. Here, we test whether this source of "big data" can be used to approximate visitation rates. We use the locations of photographs in flickr to estimate visitation rates at 836 recreational sites around the world, and use information from the profiles of the photographers to derive travelers' origins. We compare these estimates to empirical data at each site and conclude that the crowd-sourced information can indeed serve as a reliable proxy for empirical visitation rates. This new approach offers opportunities to understand which elements of nature attract people to locations around the globe, and whether changes in ecosystems will alter visitation rates.},
