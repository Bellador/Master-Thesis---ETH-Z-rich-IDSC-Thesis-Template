journal = {International Journal of Geographical Information Science},
keywords = {Place descriptions,geographic information retrieval,landscape character assessments,text processing,user-generated content},
number = {8},
pages = {1572--1592},
publisher = {Taylor {\&} Francis},
title = {{Describing and comparing landscapes using tags, texts, and free lists: an interdisciplinary approach}},
volume = {32},
year = {2018}
}
