@article{Africa2018,
abstract = {The final version of this article will appear in 2018 in a special issue of: Africa {\&} Science (http://africa-and-science.com/), a weekly Newspaper devoted to Science {\&} Technology in Africa. Tentative title of the issue: 'Self-reliance in safe drinking water provision in Africa: An appropriate solution' "We shall not defeat any of the infectious diseases that plague the developing world until we have also won the battle for safe drinking water, sanitation, and basic health care" Kofi Annan "Although the science behind is seemingly simple, biochar-based water filters represent the untapped potential frugal technology for universal clean drinking water provision". The author "Yet again, African governments in their individual and collective capacities, engage in their predictable and casual 'Wait-and-see' and 'Let-external donors-fund-it' attitude". Abstract Sub-Saharan Africa has not met the United Nations water-related Millennium Development Goals (MDGs) for safe drinking water. This is a pity given water's crucial role in accomplishing the continent's development goals. It is broadly accepted that Africa faces endemic poverty and pervasive under-development. It is also almost univocally accepted that all these countries lack the human, economic (and institutional) capacities to sustainably develop and manage their water resources. This view is unfortunately believed by some Africans. The question arises why Northern Africans, Europeans, Americans and other are more skilled than Sub-Saharan Africans? This contribution demonstrates that African natives are leading in developing two frugal technologies for safe drinking water, with proven extension capacity to sanitation. If these technologies are developed hand-in-hand, Africa can achieve the Sustainable Development Goal 6 (ensuring availability and sustainable management of water and sanitation) within the remaining 13 years. The first technology used biochar, and the second metallic iron in filtrations systems. Both technologies have demonstrated strong capacity to remove several aqueous pollutants including pathogens (e.g., bacteria, viruses), inorganic chemicals (e.g., arsenic, fluoride, nitrate, uranium) and organics (e.g. pesticides, pharmaceutical drug residues). Both technologies have several merits},
author = {Gwenzi, Willis},
file = {:C$\backslash$:/Users/Max/Documents/Master{\_}Thesis/Literatur/Gwenzi2018-FrugalWaterTreatmentfinalPersonalversion.pdf:pdf},
journal = {Researchgate.Net},
number = {January},
pages = {1--15},
title = {{Biochar-Based Filters: A Frugal Technology for Drinking Water Supply in Developing Countries?}},
url = {https://www.researchgate.net/profile/Willis{\_}Gwenzi/publication/323337472{\_}Biochar-Based{\_}Filters{\_}A{\_}Frugal{\_}Technology{\_}for{\_}Drinking{\_}Water{\_}Supply{\_}in{\_}Developing{\_}Countries/links/5a8ea25c458515eb85ad6556/Biochar-Based-Filters-A-Frugal-Technology-for-Drinking-Wate},
