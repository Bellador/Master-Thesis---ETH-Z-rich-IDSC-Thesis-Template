journal = {Biological Conservation},
keywords = {European seabass,Recreational fishing,Social media,Text and data mining,Wildlife recreation},
number = {September},
pages = {89--99},
publisher = {Elsevier},
title = {{Text and data mining of social media to map wildlife recreation activity}},
url = {https://doi.org/10.1016/j.biocon.2018.10.010},
volume = {228},
year = {2018}
}
