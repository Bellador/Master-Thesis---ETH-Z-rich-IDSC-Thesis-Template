keywords = {Biathlon world championships,Event tourism,Experiences,Global positioning systems (GPS),Space,Sweden,Time,Visitor flows},
number = {1},
pages = {1--20},
title = {{Time and space in event behaviour: Tracking visitors by GPS}},
volume = {13},
year = {2011}
